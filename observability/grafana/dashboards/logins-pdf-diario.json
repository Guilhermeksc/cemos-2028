{
  "title": "Logins e Downloads por Dia",
  "uid": "logins-pdf-diario",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "1h",
  "time": {
    "from": "now-30d",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Logins totais por dia",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum by(status) (sum_over_time(rate(login_requests_total[5m])[1d:5m]))",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 80
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Downloads de PDF por dia",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum by(origem) (sum_over_time(rate(pdf_download_total[5m])[1d:5m]))",
          "legendFormat": "{{origem}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 80
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      }
    }
  ],
  "templating": {
    "list": []
  }
}
